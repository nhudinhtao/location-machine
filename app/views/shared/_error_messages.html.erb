<% if errors.any? %>
  <div id="error_explanation">
    <p>
      <%= I18n.t("errors.messages.not_saved", count: errors.count, resource: "Account")
       %>
    </p>
    <ul>
      <% errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
    </ul>
  </div>
<% end %>
